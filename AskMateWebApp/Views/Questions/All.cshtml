@using AskMateWebApp.Domain
@model List<Question>
@{
    ViewData["Title"] = "Questions";
}
<h1>@ViewData["Title"]</h1>
<ul>
    @foreach (var q in Model)
    {
        <li>
            <a asp-controller="Questions" asp-action="Get" asp-route-id="@q.Id"><h2>@q.Title</h2></a>
        </li>
    }
</ul>
